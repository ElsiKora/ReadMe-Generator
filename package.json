{
	"name": "@elsikora/readme-generator",
	"version": "1.0.1",
	"description": "CLI utility for generating README.md files",
	"keywords": [
		"cli",
		"readme",
		"anthropic",
		"typescript",
		"openai",
		"generator"
	],
	"license": "MIT",
	"author": "ElsiKora",
	"type": "module",
	"bin": {
		"@elsikora/readme-generator": "./bin/index.js"
	},
	"files": [
		"bin"
	],
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "npm run prebuild && rollup -c",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint": "eslint ./src && prettier --check .",
		"lint:fix": "eslint ./src --fix && prettier --write .",
		"patch": "changeset",
		"release": "npm install && npm run build && changeset publish",
		"start": "node dist/index.js"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.36.3",
		"@clack/prompts": "^0.9.1",
		"axios": "^1.7.9",
		"chalk": "^5.4.1",
		"cli-progress": "^3.12.0",
		"commander": "^13.1.0",
		"dotenv": "^16.4.7",
		"fs-extra": "^11.3.0",
		"glob": "^10.4.5",
		"https-proxy-agent": "^7.0.6",
		"ignore": "^7.0.3",
		"openai": "^4.82.0",
		"rimraf": "^6.0.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.12",
		"@elsikora/eslint-config": "^3.2.4",
		"@eslint-react/eslint-plugin": "^1.26.1",
		"@eslint/compat": "^1.2.6",
		"@eslint/js": "^9.19.0",
		"@rollup/plugin-typescript": "^12.1.2",
		"@stylistic/eslint-plugin": "^3.0.1",
		"@types/cli-progress": "^3.9.2",
		"@types/eslint__js": "^8.42.3",
		"@types/fs-extra": "^11.0.4",
		"@types/glob": "^8.1.0",
		"@types/node": "^22.13.0",
		"@types/yargs": "^17.0.33",
		"@typescript-eslint/eslint-plugin": "^8.22.0",
		"@typescript-eslint/parser": "^8.23.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-check-file": "^3.0.0",
		"eslint-plugin-jsonc": "^2.19.1",
		"eslint-plugin-n": "^17.15.1",
		"eslint-plugin-ng-module-sort": "^1.3.1",
		"eslint-plugin-package-json": "^0.21.1",
		"eslint-plugin-perfectionist": "^4.7.0",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-regexp": "^2.7.0",
		"eslint-plugin-sonarjs": "^3.0.1",
		"eslint-plugin-tailwindcss": "^3.18.0",
		"eslint-plugin-typeorm-typescript": "^0.5.0",
		"eslint-plugin-unicorn": "^56.0.1",
		"eslint-plugin-yml": "^1.16.0",
		"prettier": "^3.4.2",
		"rollup": "^4.34.1",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.22.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
